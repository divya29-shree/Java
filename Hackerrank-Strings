4.  import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
         String str1=sc.nextLine();
        if(str.equals(str1)){
            System.out.println("Celestial Confirmation! The strings align.");
        } else{
            System.out.println("Galactic Discrepancy! The strings do not align.");
        }
    }    
}

