package Assignment4;
import java.util.*;
public class Palindrome {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String str=sc.nextLine();
		String rev = ""; 
        for(int i = str.length() - 1; i >= 0; i--)
            rev += str.charAt(i);
		if(str.equals(rev)) {
			System.out.println(" Palindrome");
		}
		else {
			System.out.println(" Not a Palindrome");
		}
	}}
2.Fibonacci series
package Assignment4;
import java.util.Scanner;
public class Fibbonacci {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int a=0;
		int b=1;
		System.out.print("Fibonacci Series: " + a + " " + b);
		for(int i=2;i<num;i++) {
			int c=a+b;
			System.out.print(" " + c);
			a=b;
			b=c;
		}
	}
}
3.Player score
package Assignment4;
import java.util.*;
public class Playerscore {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int t=sc.nextInt();
		int y=0,z=0;
		boolean turn=true;
		while(t>0) {
			int p=(turn)?a:b;
			int q=t;
			while(q!=0) {
				int temp=q;
				q=p%q;
				p=temp;
			}
			if(turn) {
				y+=p;
			}else {
				z+=p;
			}
			t-=p;
			turn=!turn;
		}
		System.out.println(y>z?"winner:Gokul":z>y?"winner:Minor":"It is draw.");
		}
	}

4.Armstrong number
package Day5;
import java.util.*;
public class Armstrongnumber {
	public static void main(String[] args) {
	 Scanner sc=new Scanner(System.in);
	 int num=sc.nextInt();
	 int digit=num;
	 int sum=0;
	 int count=0;
	 while(num>0) {
		 int num1=num%10;
		count++;
		 num=num/10;
	 } while(num>0) {
		 int num2=num%10; 
	 }
	}
}







	


